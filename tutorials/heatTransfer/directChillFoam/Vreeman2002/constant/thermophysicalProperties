FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;

    transport	    tabulated;
    thermo          hIcoTabulated;
    equationOfState icoTabulated;

    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        nMoles      1;
        molWeight   26.981539;
    }

    equationOfState
    {
        R           3000;
        rho
        {
            values
            (
                (  1.0000    2490.0)
                (  9999.0    2490.0)
            );
        }
    }

    thermodynamics
    {
        Hf          0;
        Sf          0;
        Cp
        {
            values
            (
                (  1.0000    1030.0)
                (  9999.0    1030.0)
            );
        }
    }

    transport
    {
        mu
        {
            type		table;
            format		foam;
	        file		"constant/mu";
            outOfBounds         clamp;
            interpolationScheme linear;
        }

        kappa
        {
            type		table;
            format		foam;
	        file		"constant/kappa";
            outOfBounds         clamp;
            interpolationScheme linear;
        }

        Pr          0.0175;
    }
}

