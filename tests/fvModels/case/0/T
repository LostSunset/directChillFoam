FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

tInitial    943.48;
waterTemp   358.0;

dimensions      [0 0 0 1 0 0 0];

internalField   uniform $tInitial;

boundaryField
{
    "(hot-top|ceramic|graphite|mould|air-gap|water-film|free-surface|ram)"
    {
        type            zeroGradient;
    }

    symmetry_planes
    {
        type            symmetry;
    }
}

